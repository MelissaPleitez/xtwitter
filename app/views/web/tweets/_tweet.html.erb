<div id="<%= dom_id tweet %>">
  <p>
    <strong>Body:</strong>
    <%= tweet.body %>
  </p>

  <p>
    <strong>User:</strong>
    <%= tweet.user_id %>
  </p>

  <p>
    <strong>Retweet:</strong>
    <%= tweet.retweet_id %>
  </p>

  <p>
    <strong>Quotes:</strong>
    <%= tweet.quote_id %>
  </p>

  <p>
    <strong>Parent tweet:</strong>
    <%= tweet.parent_tweet_id %>
  </p>

</div>